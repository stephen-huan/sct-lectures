class MinStack:

    def __init__(self, f=min): self.stk, self.f = [], f

    def empty(self): return len(self.stk) == 0

    def append(self, val):
      self.stk.append((val, \
      self.f(val, self.stk[-1][1]) if not self.empty() else val))

    def pop(self): return self.stk.pop()[0]

    def min(self): return self.stk[-1][1]
